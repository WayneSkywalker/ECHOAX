<% include ./partials/header %>
    <title>(ADMIN)USER's REQUEST</title>
</head>
<body background="https://i.imgur.com/Fg7WsNm.png" class="bg">
    <% include ./partials/navbar %>
    <div id="headca">
        <center> USER'S ECHO</center>
    </div>
    <% for(let i = 0;i < news.length; i++) { %>
        <div class="column">
            <a href="userecho/<%= news[i].id %>" style="text-decoration: none;">
                <% if(news[i].category ==='fakenews'){ %> 
                    <div class="card flex-row flex-wrap backp">
                <% } else if(news[i].category ==='hoax'){ %> 
                    <div class="card flex-row flex-wrap backhoax"> 
                <% } else { %> 
                    <div class="card flex-row flex-wrap backphish"> 
                <% } %>
                    <img class="card-img-left" src="https://i.imgur.com/TBu6Oc0.jpg" width="200px" height="150px" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title black"><%= news[i].title %></h5>
                        <p class="card-text black"><%= news[i].content.substring(0,120) + " ..." %></p>
                    </div>
                </div>
            </a>
        </div>
    <% } %>
